{
  "expo": {
    "name": "GeoFrame",
    "slug": "GeoFrame",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/image/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/image/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ndretx.GeoFrame"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/image/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "config": {
          "googleMaps": {
            "apiKey": "AIzaSyADU3bOiJGUeUU2_OHylHLwnM-Djo1UQGM"
          }
        },
        "permissions": [
          "CAMERA",
          "READ_EXTERNAL_STORAGE",
          "WRITE_EXTERNAL_STORAGE",
          "CAMERA_ROLL",
          "LOCATION",
          "locationAlwaysAndWhenInUsePermission",
          "photosPermission",
          "savePhotosPermission"
        ]
      },
      "package": "ndretx.GeoFrame"
    },
    "web": {
      "favicon": "./assets/image/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a0cae348-34c6-4fc4-9d03-70757bca217e"
      }
    }
  }
}
